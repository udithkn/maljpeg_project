{
    "name": "Mal JPG Finder",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "downloads",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
       "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "fixR.js"
        ]
    }
}